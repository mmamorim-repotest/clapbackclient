{
  "name": "clapbackclient",
  "description": "Browser/node client to clapbackdb",
  "version": "0.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "node src/test.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "build": "rm -fr dist/* && tsc && tsc -p tsconfig-mjs.json && node src/make-modules-packages.js",
    "build2": "rm -fr dist/* && tsc",
    "build3": "rm -fr dist/* && tsc -p tsconfig-mjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmamorim-repotest/clapbackclient.git"
  },
  "author": "Marcelo Amorim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmamorim-repotest/clapbackclient/issues"
  },
  "homepage": "https://github.com/mmamorim-repotest/clapbackclient#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.29",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "socket.io-client": "^4.5.0"
  }
}
